node n7 {
    type wlan
    network-config {
	hostname wlan7
	!
	interface wireless
	 ip address 10.0.4.0/24
	 ipv6 address 2001:4::0/92
	!
	mobmodel
	coreapi
	basic_range
	!
    }
    custom-config {
	custom-config-id basic_range
	custom-command {3 3 9 9 9}
	config {
	range=275
	bandwidth=54000000
	jitter=0.0
	delay=20000
	error=0.0
	}
    }
    canvas c1
    iconcoords {310.0 139.0}
    labelcoords {310.0 171.0}
    interface-peer {e0 n1}
    interface-peer {e1 n5}
    interface-peer {e2 n8}
    interface-peer {e3 n9}
}

