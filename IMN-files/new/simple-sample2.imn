node n1 {
    type router
    model router
    network-config {
	hostname n1
	!
	interface eth1
	 ip address 10.0.1.1/24
	 ipv6 address 2001:1::1/64
	 mac address 00:00:00:aa:00:0e
	!
	interface eth0
	 ip address 10.0.0.1/24
	 ipv6 address 2001::1/64
	 mac address 00:00:00:aa:00:0d
	!
	interface eth2
	 ip address 10.0.4.1/24
	 ipv6 address 2001:4::1/92
	 mac address 00:00:00:aa:00:14
	!
    }
    canvas c1
    iconcoords {299.0 222.0}
    labelcoords {299.0 254.0}
    custom-image /usr/local/share/core/icons/normal/router.gif
    services {OLSRORG IPForward}
    interface-peer {eth2 n7}
    interface-peer {eth0 n4}
    interface-peer {eth1 n2}
}

