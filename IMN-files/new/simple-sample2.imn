node n4 {
    type router
    model PC
    network-config {
	hostname wiredLaptop
	!
	interface eth0
	 ip address 10.0.0.20/24
	 ipv6 address 2001::20/64
	 mac address 00:00:00:aa:00:0c
	!
    }
    canvas c1
    iconcoords {166.0 159.0}
    labelcoords {166.0 191.0}
    custom-image /usr/local/share/core/icons/normal/pc.gif
    services {OLSRORG DefaultRoute pcap}
    interface-peer {eth0 n1}
    custom-config {
	custom-config-id service:pcap:pcap.sh
	custom-command pcap.sh
	config {
	#!/bin/sh
	# set tcpdump options here (see 'man tcpdump' for help)
	# (-s snap length, -C limit pcap file length, -n disable name resolution)
	DUMPOPTS="-n"
	
	if [ "x$1" = "xstart" ]; then
	
	tcpdump ${DUMPOPTS} -w wired-laptop.eth0.pcap -i eth0 < /dev/null &
	
	
	elif [ "x$1" = "xstop" ]; then
	    mkdir -p /home/seraph/Desktop/tshark_output/pcap
	    mv *.pcap /home/seraph/Desktop/tshark_output/pcap
	fi;
	}
    }
    custom-config {
	custom-config-id service:pcap
	custom-command pcap
	config {
	dirs=()
	files=('pcap.sh',)
	startidx=1
	cmdup=('sh pcap.sh start',)
	cmddown=('sh pcap.sh stop',)
	cmdval=('pidof tcpdump',)
	meta=logs network traffic to pcap packet capture files
	starttime=
	}
    }
}

