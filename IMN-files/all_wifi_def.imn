node n9 {
    type wlan
    network-config {
	hostname wlan9
	!
	interface wireless
	 ipv6 address 2001:4::0/128
	 ip address 10.0.4.0/24
	!
	mobmodel
	coreapi
	emane_rfpipe
	!
    }
    custom-config {
	custom-config-id emane_rfpipe
	custom-command {11 3 11 2 10 9 9 10 11 4 4 4 2 9 9 9 9 9 10 10 11 11 11 11 10}
	config {
	enablepromiscuousmode=0
	datarate=1M
	flowcontrolenable=0
	flowcontroltokens=10
	pcrcurveuri=/usr/share/emane/models/rfpipe/xml/rfpipepcr.xml
	jitter=0.0
	delay=0.0
	transmissioncontrolmap=
	enabletighttiming=0
	bandwidth=1M
	frequency=2.347G
	frequencyofinterest=2.347G
	subid=1
	systemnoisefigure=4.0
	txpower=0.0
	antennagain=0.0
	antennaazimuth=0.0
	antennaelevation=0.0
	antennaprofileid=1
	antennaprofilemanifesturi=
	antennaprofileenable=0
	defaultconnectivitymode=1
	frequencyofinterestfilterenable=1
	noiseprocessingmode=0
	pathlossmode=2ray
	}
    }
    custom-config {
	custom-config-id basic_range
	custom-command {3 3 9 9 9}
	config {
	range=275
	bandwidth=54000000
	jitter=0
	delay=20000
	error=0
