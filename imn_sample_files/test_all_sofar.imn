node n1 {
    type wlan
    network-config {
	hostname wlan1
	!
	interface wireless
	 ipv6 address 2001:2::0/128
	 ip address 10.0.2.0/24
	!
	mobmodel
	coreapi
	basic_range
	!
    }
    custom-config {
	custom-config-id basic_range
	custom-command {3 3 9 9 9}
	config {
	range=275
	bandwidth=65000000
	jitter=5000
	delay=20000
	error=0.2
	}
    }
    canvas c1
    iconcoords {161.0 35.0}
    labelcoords {161.0 67.0}
    interface-peer {e0 n4}
    interface-peer {e1 n3}
    interface-peer {e2 n2}
}

